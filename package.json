    "@types/node": "^16.11.39",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "env-cmd": "^10.1.0",
    "radium": "^0.26.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-toastify": "^9.0.4",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.3",
    "simple-react-validator": "^1.6.1",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start-dev": "env-cmd -f ./environment/.env.developement react-scripts start",
    "build-dev": "env-cmd -f ./environment/.env.developement react-scripts build && cp web.config build",
    "start-pro": "env-cmd -f ./environment/.env.production react-scripts start",
    "build-pro": "env-cmd -f ./environment/.env.production react-scripts build && cp web.config build",
    "start-qa": "env-cmd -f ./environment/.env.qa react-scripts start",
    "build-qa": "env-cmd -f ./environment/.env.qa react-scripts build && cp web.config build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "prettier": "^2.6.2"
  }
}
